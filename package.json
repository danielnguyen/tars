{
  "name": "node-ts-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate code for TypeScript projects",
  "main": "src/app.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "npm-run-all generate:swagger generate:routes",
    "generate:routes": "tsoa routes",
    "generate:swagger": "tsoa swagger",
    "clean": "rimraf coverage build tmp",
    "start": "./node_modules/.bin/ts-node ./src/app.ts",
    "start:watch": "nodemon --exec ./node_modules/.bin/ts-node -- ./src/app.ts",
    "lint": "tslint -t stylish --project \"tsconfig.json\"",
    "test": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danielnguyen/node-ts-boilerplate.git"
  },
  "author": "Daniel Nguyen",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/danielnguyen/node-ts-boilerplate/issues"
  },
  "homepage": "https://github.com/danielnguyen/node-ts-boilerplate#readme",
  "devDependencies": {
    "@types/node": "^8.0.47",
    "jest": "^21.2.1",
    "nodemon": "^1.12.1",
    "rimraf": "^2.6.2",
    "ts-node": "^3.3.0",
    "tslint": "^5.8.0",
    "tslint-microsoft-contrib": "^5.0.1",
    "typescript": "^2.6.1"
  },
  "engineStrict": true,
  "engines": {
    "node": ">= 8.9.0"
  },
  "dependencies": {
    "@types/body-parser": "^1.16.8",
    "@types/dotenv": "^4.0.2",
    "@types/express": "^4.0.39",
    "@types/morgan": "^1.7.35",
    "@types/swagger-tools": "^0.10.5",
    "body-parser": "^1.18.2",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "fs": "0.0.1-security",
    "http": "0.0.0",
    "http-status-codes": "^1.3.0",
    "https": "^1.0.0",
    "morgan": "^1.9.0",
    "npm-run-all": "^4.1.2",
    "path": "^0.12.7",
    "swagger-tools": "^0.10.3",
    "swagger-ui": "^3.6.0",
    "tsoa": "^2.0.1",
    "yamljs": "^0.3.0"
  }
}
